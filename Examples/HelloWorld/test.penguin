                namespace ns {
                    initial {
                        var test : Test<Foo> = Test<Foo>.a();
                        if (test is Test<Foo>.a) {
                            print("a");
                        }
                        test = Test<Foo>.b(new Foo());
                        if (test is Test<Foo>.b) {
                            print(test.b.x as string);
                            test.b.x = 1;
                            print(test.b.x as string);
                        } else if (test is Test<Foo>.a) {
                            print("not possible");
                        }
                    }

                    class Foo {
                        var x: u8 = 0;
                    }

                    enum Test <T> {
                        a;
                        b : T;
                    }
                }